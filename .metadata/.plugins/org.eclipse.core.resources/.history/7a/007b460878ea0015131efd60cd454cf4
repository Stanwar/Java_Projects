package Beverages;

public class Cream extends Condiments{
	@Override
	public float price(){
		return 0.25f;
	}
	@Override
	public String name(){
		return "Cream";
	}
	@Override
	public void setQuantity(int number){
		this.number = number;
	}
	@Override
	public int getQuantity(){
		return number;
	}
}
