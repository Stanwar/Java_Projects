package Beverages;

public class Expresso extends Coffees{
	private int number =10;
	@Override
	public float price(){
		return 1.10f;
	}
	public String name(){
		return "Expresso";
	}
	@Override
	public void setQuantity(int number){
		this.number = number;
	}
	@Override
	public int getQuantity(){
		return number;
	}
}
