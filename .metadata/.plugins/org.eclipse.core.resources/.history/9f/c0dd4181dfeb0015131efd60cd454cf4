package Beverages;

public class Sugar extends Condiments{
	private int number = 10;
	@Override
	public float price(){
		return 0.25f;
	}
	
	@Override
	public String name(){
		return "Sugar";
	}
	@Override
	public void setQuantity(int number){
		this.number = number;
	}
	@Override
	public int getQuantity(){
		return number;
	}
}
